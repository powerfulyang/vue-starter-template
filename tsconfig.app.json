{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "#/*": ["./*"]
    },
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "vueCompilerOptions": {
    "plugins": ["unplugin-vue-macros/volar"]
  },
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "openapi/*"
  ],
  "exclude": ["src/**/__tests__/*"]
}
